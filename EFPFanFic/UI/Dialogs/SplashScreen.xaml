<Window x:Class="EFPFanFic.UI.Dialogs.SplashScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dtMvvm="clr-namespace:EFPFanFic.UI.Dialogs.ViewModel.DesignTime"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance dtMvvm:DTSplashScreenViewModel, IsDesignTimeCreatable=True}"
        Title="SplashScreen" Height="146.476" Width="238.952"
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None" Topmost="True">
    <Grid>
        <Image HorizontalAlignment="Left" Height="70" Margin="10,10,0,0" VerticalAlignment="Top" Width="212" Source="/EFPFanFic;component/Images/logoprim.png"/>
        <Label Content="{Binding Message}" Margin="10,0,10,8" Height="31" VerticalAlignment="Bottom" VerticalContentAlignment="Center"/>
        <Label Content="{Binding Version}" Margin="22,80,0,0" Height="25" VerticalAlignment="Top" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="200"/>


    </Grid>
</Window>
